{
  "name": "at-monorepo-test",
  "version": "0.0.1",
  "main": "./index.js",
  "license": "MIT",
  "private": true,
  "repository": "git+https://github.com/asos-albinotonnina/tech-develop-monorepo-versioning.git",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "release": "dotenv -- cross-var yarn workspaces run semantic-release -e semantic-release-monorepo && git add . && git push --tags --tag $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "dotenv-cli": "^5.1.0"
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-eslint": "^3.0.9",
    "semantic-release": "^19.0.2",
    "semantic-release-monorepo": "^7.0.5"
  }
}